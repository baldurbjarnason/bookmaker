<!DOCTYPE HTML>
<html lang="{{ meta.lang }}">
<head>
<title>Table of Contents – {{ meta.title }}</title>
{{#if meta.specifiedCss }}{{#each css}}
<link href="{{relative ../filename this }}" type="text/css" rel="stylesheet">
{{/each}}{{else}}
{{#each assets.css}}
<link href="{{relative ../filename this }}" type="text/css" rel="stylesheet">
{{/each}}{{/if}}{{#if meta.specifiedJs }}{{#each js}}
<script src="{{relative ../filename this }}" type="application/javascript"></script>
{{/each}}{{else}}
{{#each assets.js}}
<script src="{{relative ../filename this }}" type="application/javascript"></script>
{{/each}}{{/if}}{{#each links}}
<link href="{{ this.href }}" type="{{ this.type }}" rel="{{ this.rel }}"{{#if this.hreflang }} hreflang="{{ this.hreflang }}"{{/if}}{{#if this.title }} title="{{ this.title }}"{{/if}} />
{{/each}}
</head>
<body class="nav">
<h1>{{ meta.title }} – Table of Contents</h1>
<nav id="toc" class="toc h-toc">
<ol>
    {{{ navList }}}
</ol>
</nav>
{{#if meta.copyrightStatement}}
<p class="copyright">{{ meta.copyrightStatement }}</p>
{{else}}
<p class="copyright">Copyright &#xa9; {{ meta.copyrightYear }} {{ meta.author }} {{#if meta.author2 }}and {{ meta.author2 }}{{/if}}</p>
{{/if}}
</body>
</html>